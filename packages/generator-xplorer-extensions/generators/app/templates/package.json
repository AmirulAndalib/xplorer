{
	"name": "<%= extensionIdentifier %>",
	"extensionName": "<%= extensionName %>",
	"description": "My Extension Description",
	"category": "<%= extensionType %>",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"start": "xplorer --theme <%= extensionIdentifier %>",
		"build": "xplorer build-extension themes"
	},
	"xplorerExtensionConfig": {
		"themes": [
			{
				"identifier": "<%= extensionIdentifier %>",
				"name": "<%= extensionName %>",
				"category": "<%= themeCategory %>",
				"path": "./themes/<%= extensionIdentifier%>-color-theme.json"
			}
		]
	}
}
